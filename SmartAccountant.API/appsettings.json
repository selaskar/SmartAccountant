{
  "AzureAd": {
    "Instance": "https://smartaccountant.ciamlogin.com/",
    "TenantId": "set in secrets",
    "ClientId": "set in secrets"
  },
  "Identity": {
    "TenantId": "set in secrets",
    "CredentialType": "ManagedIdentity"
  },
  "AzureStorage": {
    "ServiceAddress": "set in secrets"
  },
  "CoreDatabase": {
    "ConnectionString": "set in secrets"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Error",
      "Microsoft.EntityFrameworkCore": "None", // EF independently and duplicately logs errors.
      "SmartAccountant": "Information"
    }
  },
  "AzureMonitor": {
    "ConnectionString": "set in secrets"
  },
  "AllowedHosts": "*"
}